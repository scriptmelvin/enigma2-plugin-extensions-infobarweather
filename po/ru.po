#              it will be replaced by the plugins version
msgid "InfoBarWeather %(version)s setup"
msgstr "Настройки InfoBarWeather %(version)s"

msgid "Enabled"
msgstr "Включить"

msgid "Location"
msgstr "Местоположение"

msgid "Press OK to open location search."
msgstr "Нажмите OK, чтобы открыть поиск местоположения."

msgid "Wind speed unit"
msgstr "Единица измерения скорости ветра"

msgid "Display wind speed as BFT, m/s, km/h or mph."
msgstr "Отобразить скорость ветра в виде Ш.Б, м/с, км/ч или м/ч."

msgid "Temperature unit"
msgstr "Единица измерения температуры"

msgid "Display temperature as °C or °F."
msgstr "Отобразить температуры в °C или °F."

msgid "Show location"
msgstr "Показать местоположение"

msgid "Show last update time"
msgstr "Показать время последнего обновления"

msgid "Show last update time by weather service."
msgstr "Показать время последнего обновления по метеослужбе."

msgid "Show sunrise/sunset"
msgstr "Показать рассвет/закат"

msgid "Show humidity"
msgstr "Показать влажность"

msgid "Show rain"
msgstr "Показать осадки"

msgid "Show rain forecast"
msgstr "Показать прогноз осадков"

msgid ""
"Show two hour rain forecast instead of rain probability (only available for The Netherlands and Belgium)."
msgstr ""
"Показывать двухчасовой прогноз осадков вместо вероятности осадков (доступно только для Нидерланды и Бельгии)."

msgid "Show wind"
msgstr "Показать скорость ветера"

msgid "Show icon"
msgstr "Показать иконки"

msgid "Show temperature"
msgstr "Показать температуру"

msgid "Show feel temperature"
msgstr "Показать температуру по ощущению"

msgid "Save"
msgstr "Сохранить"

msgid "Enter (part of) location to search for (e.g. \"Amsterdam\" or \"Ams\"):"
msgstr "Введите (часть) местоположение для поиска,(пример: \"Amsterdam\" или \"Ams\"):"

msgid "Could not download location data."
msgstr "Не удалось загрузить данные о местоположении."

msgid "Show current weather in infobar"
msgstr "Показать текущую погоду в инфобаре"

